{
    "name": "stem-achievements-data",
    "version": "1.0.0",
    "description": "Test data package containing achievements of women, non-binary, queer, black, indigenous and people of colour in STEM",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:image": "vitest run --config vitest.image.config.ts",
        "dev": "tsx src/index.ts",
        "generate-diversity": "node scripts/generate-diversity-report.js"
    },
    "keywords": [
        "test-data",
        "stem",
        "women-in-stem",
        "diversity-in-science",
        "scientists",
        "engineers",
        "mathematicians"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0",
        "vitest": "^1.0.0"
    },
    "peerDependencies": {
        "test-data-factory": "file:../test-data-api"
    }
}